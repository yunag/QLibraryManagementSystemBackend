{
  "root": true,
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 13
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "react/display-name": 0
  }
}
